{
    "projectId": "basic7challenge",
    "baseUrl": "https://www.mercedes-benz.co.uk",
    "defaultCommandTimeout": 30000,
    "viewportWidth": 1920,
    "viewportHeight": 1080,
    "video": true,
    "videoCompression": false,
    "includeShadowDom": true,
    "reporter": "mochawesome",
    "reporterOptions": {
        "reportDir": "report",
        "overwrite": true,
        "html": true,
        "json": true,
        "charts": true,
        "pretty": true
    },
    "experimentalSessionAndOrigin": false,
    "retries": {
        "runMode": 0,
        "openMode": 0
    },
    "testFiles": [
        "**/*.feature",
        "**/*.spec.js"
    ],
    "numTestsKeptInMemory": 0
}